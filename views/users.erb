<div class="welcomeHome">
  <h1>Welcome</h1>
  <% if current_user %>
      <h2>Welcome back <%= current_user.fname %>!</h2>
  <% end %>

<h1>LIST OF USERS</h1>
<% @users.each do |user| %>
    <h4><a href="/user/<%= user.id %>"><%= user.fname %> <%= user.lname %></a></h4>
    <p>Age: <%= user.age %></p>
<% end %>
